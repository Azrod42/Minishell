################################################################################################# builtin/env ##################################################################################################
env\n                                                                                                                                                                                                      
[SETUP export A=a] env\n                                                                                                                                                                                   
[SETUP export A=a B=b C=c] env\n                                                                                                                                                                           
[SETUP export A=a B=b C=c] env | cat -e\n                                                                                                                                                                  
Env\n                                                                                                                                                                                                      
enV\n                                                                                                                                                                                                      
[EXPORTS PATH='/bin:/usr/bin'] Env\n                                                                                                                                                                       
[EXPORTS PATH='/bin:/usr/bin'] enV\n       
