################################################################################################# builtin/pwd ##################################################################################################
pwd\n                                                                                                                                                                                                      
[SETUP cd ..] pwd\n                                                                                                                                                                                        
[SETUP cd ../..] pwd\n                                                                                                                                                                                     
[SETUP cd ../../..] pwd\n                                                                                                                                                                                  
[SETUP cd /] pwd\n                                                                                                                                                                                         
[SETUP cd $HOME] pwd\n                                                                                                                                                                                     
pwd | cat -e\n                                                                                                                                                                                             
[EXPORTS PWD='/etc'] pwd\n                                                                                                                                                                                 
unset PWD; pwd; echo $PWD\n                                                                                                                                                                                
export PWD=foo; pwd; echo $PWD\n                                                                                                                                                                           
Pwd\n                                                                                                                                                                                                      
pwD\n                                                                                                                                                                                                      
[EXPORTS PATH='/bin:/usr/bin'] Pwd\n                                                                                                                                                                       
[EXPORTS PATH='/bin:/usr/bin'] pwD\n                   
