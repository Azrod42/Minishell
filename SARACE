################################################################################################## builtin/cd ##################################################################################################
echo $PWD; echo $OLDPWD; cd .; pwd; echo $PWD; echo $OLDPWD\n                                                                                                                                              
echo $PWD; echo $OLDPWD; cd ..; pwd; echo $PWD; echo $OLDPWD\n                                                                                                                                             
echo $PWD; echo $OLDPWD; cd ../..; pwd; echo $PWD; echo $OLDPWD\n                                                                                                                                          
echo $PWD; echo $OLDPWD; cd ../../..; pwd; echo $PWD; echo $OLDPWD\n                                                                                                                                       
echo $PWD; echo $OLDPWD; cd ../../../..; pwd; echo $PWD; echo $OLDPWD\n                                                                                                                                    
echo $PWD; echo $OLDPWD; cd ../../../../..; pwd; echo $PWD; echo $OLDPWD\n                                                                                                                                 
echo $PWD; echo $OLDPWD; cd ../../../../../..; pwd; echo $PWD; echo $OLDPWD\n                                                                                                                              
echo $PWD; echo $OLDPWD; cd /; pwd; echo $PWD; echo $OLDPWD\n                                                                                                                                              
echo $PWD; echo $OLDPWD; cd ''; pwd; echo $PWD; echo $OLDPWD\n                                                                                                                                             
echo $PWD; echo $OLDPWD; cd '' ''; pwd; echo $PWD; echo $OLDPWD\n                                                                                                                                          
echo $PWD; echo $OLDPWD; cd '' '' ''; pwd; echo $PWD; echo $OLDPWD\n                                                                                                                                       
echo $PWD; echo $OLDPWD; cd ' '; pwd; echo $PWD; echo $OLDPWD\n                                                                                                                                            
[SETUP mkdir d] echo $PWD; echo $OLDPWD; cd d ''; pwd; echo $PWD; echo $OLDPWD\n                                                                                                                           
[SETUP mkdir d] echo $PWD; echo $OLDPWD; cd d d; pwd; echo $PWD; echo $OLDPWD\n                                                                                                                            
[SETUP mkdir d] echo $PWD; echo $OLDPWD; cd d ' '; pwd; echo $PWD; echo $OLDPWD\n                                                                                                                          
[EXPORTS HOME='/Users/tom'] echo $PWD; echo $OLDPWD; cd $HOME; pwd; echo $PWD; echo $OLDPWD\n                                                                                                              
echo $PWD; echo $OLDPWD; cd /; pwd; echo $PWD; echo $OLDPWD\n                                                                                                                                              
echo $PWD; echo $OLDPWD; cd /.; pwd; echo $PWD; echo $OLDPWD\n                                                                                                                                             
echo $PWD; echo $OLDPWD; cd /./; pwd; echo $PWD; echo $OLDPWD\n                                                                                                                                            
echo $PWD; echo $OLDPWD; cd /././././; pwd; echo $PWD; echo $OLDPWD\n                                                                                                                                      
echo $PWD; echo $OLDPWD; cd //; pwd; echo $PWD; echo $OLDPWD\n                                                                                                                                             
echo $PWD; echo $OLDPWD; cd ///; pwd; echo $PWD; echo $OLDPWD\n                                                                                                                                            
echo $PWD; echo $OLDPWD; cd ////; pwd; echo $PWD; echo $OLDPWD\n                                                                                                                                           
echo $PWD; echo $OLDPWD; cd //////////////////////////////////////////////////////; pwd; echo $PWD; echo $OLDPWD\n                                                                                         
echo $PWD; echo $OLDPWD; cd; echo $OLDPWD\n                                                                                                                                                                
[EXPORTS HOME='/Users/tom'] echo $PWD; echo $OLDPWD; cd; echo $PWD; echo $OLDPWD\n                                                                                                                         
echo $PWD; echo $OLDPWD; cd ' /'; pwd; echo $PWD; echo $OLDPWD\n                                                                                                                                           
echo $PWD; echo $OLDPWD; cd ' / '; pwd; echo $PWD; echo $OLDPWD\n                                                                                                                                          
echo $PWD; echo $OLDPWD; cd '                  /'; pwd; echo $PWD; echo $OLDPWD\n                                                                                                                          
echo $PWD; echo $OLDPWD; cd '                  /              '; pwd; echo $PWD; echo $OLDPWD\n                                                                                                            
echo $PWD; echo $OLDPWD; cd ' // '; pwd; echo $PWD; echo $OLDPWD\n                                                                                                                                         
echo $PWD; echo $OLDPWD; cd //home; pwd; echo $PWD; echo $OLDPWD\n                                                                                                                                         
echo $PWD; echo $OLDPWD; cd ' //home'; pwd; echo $PWD; echo $OLDPWD\n                                                                                                                                      
echo $PWD; echo $OLDPWD; cd '     //home    '; pwd; echo $PWD; echo $OLDPWD\n                                                                                                                              
[SETUP mkdir -m 000 d] echo $PWD; echo $OLDPWD; cd d; echo $OLDPWD\n                                                                                                                                       
